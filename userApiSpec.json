{
    "openapi":"3.1.0",
    "info":
    {
        "title":"api wisata pandeglang",
        "description":"untuk menampilkan seluruh wisata pandeglang"
    },
    "server":
    [
        {"url":"http:://wisata.pdg.id"}
    ],

    "components":
    {
        "SecuritySchemes":
        {
            "JwtAuth":
            {
                "type":"http",
                "format":"bearer",
                "bearerScheme":"JWT"
            }
        },
        "requestBody":
        {
            "wisata":
            {
                "type":"object",
                "properties":
                {
                    "id":
                    {
                        "type":"integer",
                        "example":"1"
                    },
                    "nama_wisata":
                    {
                        "type":"string",
                        "example":"curug pandeglang"
                    },
                    "alamat_wisata":
                    {
                        "type":"string",
                        "example":"kp.carita jl.raya labuan"
                    },
                    "tiket":
                    {
                        "type":"string",
                        "example":"3000"
                    },
                    "jam_operasiona":
                    {
                        "type":"string",
                        "example":"08:00-20:00"
                    },
                    "deskripsi":
                    {
                        "type":"string",
                        "example":"wisata alam pdg"
                    },
                    "gambar":
                    {
                        "type":"file",
                        "example":"curug.png"
                    }
                }
            }
        },
        "responses":
        {
            "type":"object",
            "properties":
            {
                "failed":
                {
                    "type":"string",
                    "example":"400"
                }
            }
        }
    },
    "paths":
    {
       
        "/add/wisata":
        {
            "post":
            {
                "security":
                [
                    {"JwtAuth":[]}
                ],
            
            "parameters":
            [
                {
                    "name":"bearerToken",
                    "in":"header"
                }
            ],
            "requestBody":
            {
                "required":true,
                "description":"form untuk wisata",
                "content":
                {
                    "application/json":
                    {
                        "schema":
                        {
                            "$ref":"#/components/requestBody/wisata"
                        }
                    }
                }
            },
            "responses":
            {
                "200":
                {
                    "description":"success",
                    "content":
                    {
                        "application/json":
                        {
                            "schema":
                            {
                                "$ref":"#/components/requestBody/wisata"
                            }
                        }
                    }
                },
                "400":
                {
                    "description":"failed",
                    "content":
                    {
                        "application/json":
                        {
                            "schema":
                            {
                                "$ref":"#/components/responses"
                            }
                        }
                    }
                }
            }
        }
            }
    }
}